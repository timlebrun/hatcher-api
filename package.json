{
  "name": "@timlebrun/hatcher-api",
  "version": "0.0.1",
  "description": "Small set of tabletop RPG helping modules as a deployable API",
  "author": "Tim Lebrun <me@timlebrun.fr>",
  "scripts": {
    "tsc": "tsc",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma": "prisma",
    "assets:build": "node ./esbuild.mjs build",
    "assets:watch": "node ./esbuild.mjs watch"
  },
  "dependencies": {
    "@nestjs/common": "^9.1.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.1.2",
    "@nestjs/platform-express": "^9.1.2",
    "@nestjs/platform-ws": "^9.1.2",
    "@nestjs/swagger": "^6.1.2",
    "@nestjs/websockets": "^9.1.2",
    "@prisma/client": "^4.14.1",
    "alpinejs": "^3.12.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "edge.js": "^5.5.0",
    "eventemitter2": "^6.4.9",
    "glob": "^10.2.6",
    "jsonwebtoken": "^9.0.0",
    "luxon": "^3.3.0",
    "openid-client": "^5.4.2",
    "pg": "^8.11.0",
    "pg-listen": "^1.7.0",
    "reconnecting-websocket": "^4.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^6.5.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@types/alpinejs": "^3.7.1",
    "@types/bootstrap": "^5.2.6",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/luxon": "^3.3.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.4",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "esbuild": "^0.18.4",
    "esbuild-sass-plugin": "^2.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "prisma": "^4.14.1",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
